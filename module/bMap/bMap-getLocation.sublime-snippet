bMap.getLocation({
    accuracy: '${1:10m}',//10m/100m/1km/3km
    autoStop: ${2:true},
    filter: ${3:1}
}, function(ret, err){
    if(ret.status){
        alert(JSON.stringify(ret));
    }else{
        alert(err.code);
    }
});
